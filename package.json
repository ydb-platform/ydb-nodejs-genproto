{
  "name": "ydb-sdk-proto",
  "version": "0.0.1",
  "description": "Node.js YDB SDK compiled protobufs",
  "main": "proto/bundle.js",
  "files": [
    "proto/**"
  ],
  "scripts": {
    "build": "pbjs -t static-module -r ydb-sdk -w commonjs -p . `ls api/*.proto` yandex/cloud/iam/v1/iam_token_service.proto > proto/bundle.js && pbts -o proto/bundle.d.ts proto/bundle.js"
  },
  "keywords": [
    "ydb",
    "sdk",
    "proto"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ydb-platform/ydb-nodejs-genproto.git"
  },
  "author": "Timur Sufiev",
  "license": "Apache",
  "devDependencies": {
    "protobufjs": "^6.8.8"
  }
}
